<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.abx.jsservey"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="22"
        android:targetSdkVersion="23" />
<uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.jsservey.view.login.SplashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jsservey.view.MainActivity" ></activity>
        <activity
            android:name="com.jsservey.view.BaseActivity" ></activity>
        <activity
            android:name="com.jsservey.view.SurveyActivity" ></activity>
        <activity
            android:name="com.jsservey.view.login.LoginActivity" ></activity>
        <activity
            android:name="com.jsservey.view.login.CreateAccount" ></activity>
    </application>

</manifest>
